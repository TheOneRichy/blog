    "starter",
    "blog",
    "theme",
    "dark",
    "light",
    "personal site"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby clean && gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
